transform {
	param "filename" { select "'addressbook.sda'" }
	variable "doc" { select "document($filename)" }

	variable "total" { select "1" }
	println "concat('total: ', $total)"
	
	foreach "$doc//contact" {
		variable "total" { select "$total + 1" }
		println "concat('total: ', $total)"
	}
	
	println "concat('total: ', $total)"
}